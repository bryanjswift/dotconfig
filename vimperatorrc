set guioptions=B
autocmd DOMLoad .* js modes.passAllKeys = /www\.google\.com/.test(buffer.URL)
autocmd PageLoad .* js modes.passAllKeys = /www\.google\.com/.test(buffer.URL)
autocmd LocationChange .* js modes.passAllKeys = /www\.google\.com/.test(buffer.URL)

